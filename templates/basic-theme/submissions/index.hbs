<div class="row">
  <div class="col-md-6">
    <h2>Contributions</h2>
    <a class="add-button btn btn-lg btn-primary" href="/contributions/new"><span class="glyphicon glyphicon-plus-sign glyphicon-align-left"></span>add contribution</a>
    <ul class="contributions">
      {{#each contributions}}
        <li class="title-submission">
          <a href="/contributions/{{this.value._id}}">{{this.value.title}}</a>
        </li>
      {{/each}}
    </ul>
  </div>
  <div class="col-md-6">
    <h2>Interests</h2>
    <a class="add-button btn btn-lg btn-primary" href="/interests/new">
      <span class="glyphicon glyphicon-plus-sign glyphicon-align-left"></span>add  interest
    </a>
    <ul class="interests">
      {{#each interests}}
        <li class="title-submission">
          <a href="/interests/{{this.value._id}}">{{this.value.title}}</a>
        </li>
      {{/each}}
    </ul>
  </div>
</div>
